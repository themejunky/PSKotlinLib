<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomInput">
        <attr name="ci_title_text" format="string" />
        <attr name="ci_error_text" format="string" />
        <attr name="ci_hint_text" format="string" />
        <attr name="ci_imei_option" format="enum">
            <enum name="done" value="1" />
            <enum name="next" value="2" />
        </attr>
        <attr name="ci_lines" format="integer" />
        <attr name="ci_keyboard" format="enum">
            <enum name="text" value="1" />
            <enum name="min_text" value="12" />
            <enum name="email" value="2" />
            <enum name="phone" value="3" />
            <enum name="password" value="4" />
            <enum name="numbers" value="5" />
        </attr>


    </declare-styleable>


    <declare-styleable name="CustomSpinner">
        <attr name="cs_title_text" format="string" />
        <attr name="cs_error_text" format="string" />
    </declare-styleable>


    <declare-styleable name="CustomButton">
        <attr name="cb_text" format="string" />
        <attr name="cb_tag" format="string" />
        <attr name="cb_form_validation" format="boolean" />
        <attr name="cb_active_background" format="color" />
        <attr name="cb_active_shadow" format="color" />

    </declare-styleable>

</resources>