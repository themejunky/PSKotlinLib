<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomInput">
        <attr name="textulmeu" format="string" />
        <attr name="ci_title_text" format="string" />
        <attr name="ci_default_error_text" format="string" />
        <attr name="ci_ws_error_text" format="string" />
        <attr name="ci_hint_text" format="string" />
        <attr name="ci_text_text" format="string" />
        <attr name="ci_show_error" format="boolean" />
        <attr name="ci_tag" format="string" />
        <attr name="ci_imei_option" format="enum">
            <enum name="done" value="1" />
            <enum name="next" value="2" />
        </attr>
        <attr name="ci_lines" format="integer" />
        <attr name="ci_keyboard" format="enum">
            <enum name="text" value="1" />
            <enum name="min_text" value="12" />
            <enum name="email" value="2" />
            <enum name="phone" value="3" />
            <enum name="password" value="4" />
            <enum name="numbers" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomSpinner">
        <attr name="cs_title_text" format="string" />
        <attr name="cs_set_entries" format="reference"/>
        <attr name="cs_position" format="integer"/>
        <attr name="cs_default_error_text" format="string" />
        <attr name="cs_tag" format="string" />
        <attr name="cs_ws_error_text" format="string" />
        <attr name="cs_show_error" format="boolean" />
        <attr name="cs_transparent" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="cb_text" format="string" />
        <attr name="cb_tag" format="string" />
        <attr name="cb_form_validation" format="boolean" />
        <attr name="cb_form_loading" format="boolean" />
        <attr name="cb_color_active" format="color" />
        <attr name="cb_color_hover" format="color" />
        <attr name="cb_color_shadow" format="color" />
        <attr name="cb_enable" format="boolean" />
        <attr name="cb_button_color" format="enum">
            <enum name="blue" value="1" />
            <enum name="green" value="2" />
            <enum name="grey" value="3" />
            <enum name="red" value="4" />
            <enum name="violet" value="5" />
        </attr>

    </declare-styleable>

    <declare-styleable name="CustomButtonImageText">
        <attr name="cbuit_text" format="string" />
        <attr name="cbuit_tag" format="string" />
        <attr name="cbuit_enable" format="boolean" />
        <attr name="cbuit_icon" format="boolean"/>
        <attr name="cbuit_button_color" format="enum">
            <enum name="blue" value="1" />
            <enum name="green" value="2" />
            <enum name="grey" value="3" />
            <enum name="red" value="4" />
            <enum name="violet" value="5" />
        </attr>

    </declare-styleable>


    <declare-styleable name="CustomTextView">

        <attr name="ctv_text_color" format="enum">
            <enum name="black" value="0" />
            <enum name="blue" value="1" />
            <enum name="green" value="2" />
            <enum name="grey" value="3" />
            <enum name="red" value="4" />
            <enum name="violet" value="5" />
        </attr>
        <attr name="ctv_tag" format="string"/>
        <attr name="ctv_active_text_color" format="color" />
        <attr name="ctv_hover_text_color" format="color" />
    </declare-styleable>


    <declare-styleable name="CustomBoxImage">
        <attr name="cbi_is_selected" format="boolean" />
        <attr name="cbi_icon" format="reference" />
        <attr name="cbi_text" format="string" />
        <attr name="cbi_tag" format="string"/>
        <attr name="cbi_twin" format="integer"/>
        <attr name="cbi_active_text_color" format="color"/>
        <attr name="cbi_hover_text_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="CustomBoxImageText">
        <attr name="cbit_is_selected" format="boolean" />
        <attr name="cbit_icon" format="reference" />
        <attr name="cbit_text" format="string" />
        <attr name="cbit_tag" format="string" />
        <attr name="cbit_active_text_color" format="color" />
        <attr name="cbit_hover_text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CustomImageText">
        <attr name="cit_icon" format="reference" />
        <attr name="cit_text" format="string" />
        <attr name="cit_tag" format="string" />
        <attr name="cit_color" format="enum">
            <enum name="blue" value="1" />
            <enum name="green" value="2" />
            <enum name="grey" value="3" />
            <enum name="red" value="4" />
            <enum name="violet" value="5" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomImageGallery">
        <attr name="cig_ceva" format="boolean" />
    </declare-styleable>

</resources>